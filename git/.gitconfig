[color]
	ui = auto
	branch = auto
	status = auto
	diff = auto
[alias]
	st = status
	co = checkout
	br = branch
	mg = merge
	ci = commit
	dt = difftool
	mt = mergetool
	last = log -1 HEAD
	graph = log --oneline --pretty='%h - %s' --graph
[core]
	editor = vim
	safecrlf = false
	autocrlf = input
	ignorecase = true
	excludesfile = ~/.gitignore_global
